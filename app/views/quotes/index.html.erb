<h1>Listing quotes</h1>

<table>
  <tr>
    <th>Quote</th>
    <th>Source</th>
    <th>Link</th>
    <th>Date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @quotes.each do |quote| %>
  <tr>
    <td><%= quote.quote %></td>
    <td><%= quote.source %></td>
    <td><%= quote.link %></td>
    <td><%= quote.date %></td>
  </tr>
<% end %>
</table>

